
    <script src="js/vendor/jquery.js"></script>
    <script src="js/foundation.min.js"></script>
    <script>
      $(document).foundation();
    </script>
        <script>
        console.log("test");
      $(document).ready(function(){
        console.log("current from = " + from);
        (function poll(){
          setTimeout(function(){
           $.ajax({ url: "/event/" + from + "/5.0", success: function(data){
             $.each(data.events, function(id,v) {
               if (v[0] >= from) {
                 from = v[0] + 1;
                 console.log("new from = " + from);
                  location.reload(true);
               };
             });
             poll();
           }, dataType: "json"});
         }, 1000);
        })();
      });
        </script>
  </body>
</html>
